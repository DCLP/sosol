#main
  = render :partial => 'identifiers/edit_bar'
  .site
    %h1
      History for 
      = @identifier.title
      from publication
      = @identifier.publication.title
      %br
    %h3
      = render :partial => "identifiers/pn_link", :locals => {:identifier => @identifier}
    #edit
      %ul
        - for commit in @identifier[:commits]
          %li= link_to commit[:message], commit[:url]
